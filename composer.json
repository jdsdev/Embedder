{
  "name": "jdsdev/craft-embedder",
  "description": "Generate video embed code.",
  "version": "4.0.0",
  "type": "craft-plugin",
  "license": "MIT",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "embedder"
  ],
  "authors": [
    {
      "name": "jonathan@jdsdev.com",
      "homepage": "https://jdsdev.com/"
    }
  ],
  "require": {
    "craftcms/cms": "^5.0.0-beta.1",
    "php": "^8.2"
  },
  "autoload": {
    "psr-4": {
      "jdsdev\\embedder\\": "embedder/"
    }
  },
  "extra": {
    "name": "Embedder",
    "handle": "embedder",
    "changelogUrl": "https://raw.githubusercontent.com/jdsdev/Embedder/master/CHANGELOG.md",
    "components": {
      "embedder": "jdsdev\\embedder\\services\\EmbedderService"
    },
    "class": "jdsdev\\embedder\\Embedder"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "craftcms/rector": "dev-main"
  }
}
